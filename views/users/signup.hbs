<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>

        (function () {
            'use strict';
            window.addEventListener('load', function () {
                // Fetch all the forms we want to apply custom Bootstrap validation styles to
                var forms = document.getElementsByClassName('needs-validation');
                // Loop over them and prevent submission
                var validation = Array.prototype.filter.call(forms, function (form) {
                    form.addEventListener('submit', function (event) {
                        if (form.checkValidity() === false) {
                            event.preventDefault();
                            event.stopPropagation();
                        }
                        form.classList.add('was-validated');
                    }, false);
                });
            }, false);
        })();

    </script>
</head>

<body>
    <section>
        <div style="height:900px;background:cornsilk">
            <div class="row">
                <div class="col-md-8">
                    <div class="row text-center" style="">
                        <div class="col-md-7 text-center" style="background-color:none; margin-top:10px;">
                            <div class='product-holder'>
                                <img src="https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimg1.cookinglight.timeinc.net%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fmedium_2x%2Fpublic%2F1542062283%2Fchocolate-and-cream-layer-cake-1812-cover.jpg%3Fitok%3DrEWL7AIN"
                                    class="img-fluid responsive" alt="">

                            </div>
                        </div>
                    </div>





                </div>
                <div class="col-md-5 p-3">


                    <h1 class="pt-2"><span>Register to</span> <span
                            style="text-shadow: -1px 0 black, 0 1px black, 1px 0 black, 0 -1px black;"
                            class="text-danger font-weight-bold ">Home</span>
                        <span style="text-shadow: -1px 0 black, 0 1px black, 1px 0 black, 0 -1px black;"
                            class="text-warning">Chef</span>
                    </h1>

                    <br>



                    <form class="needs-validation mt-4" action="/signup" method="post" novalidate>
                        <div class="form-col">
                        

                                <input type="text" class="form-control" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom01" name="Fname" placeholder="First name"
                                    required>
                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                            

                            

                                <input type="text" class="form-control" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" name="Lname" placeholder="Last name"
                                    required>
                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                            


                            

                                <input type="text" class="form-control" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" name="Name" placeholder="User name"
                                    required>

                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                                <div class="invalid-feedback">
                                    name required
                                </div>
                        

                            

                                <input type="email" class="form-control" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" placeholder="Email" name="Email" required>
                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                                <div class="invalid-feedback">
                                    wrong email Id
                                </div>
                        

                            

                                <input type="text" class="form-control" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" name="Password" placeholder="password"
                                    required>
                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                                <div class="invalid-feedback">
                                    password required!
                                </div>
                        

                            

                                <input type="tel" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" class="form-control" name="phonenumber" id="validationCustom02"
                                    placeholder="Phone Number" required>
                                <div class="valid-feedback">
                                    Looks good!
                                </div>
                                <div class="invalid-feedback">
                                    wrong number!
                                </div>
                            

                            <input type="text" style=" border: none;
  border-bottom: 2px solid gray; border-width:2px;" value="true" name="blockStatus" hidden>
   <input type="text" style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" value="true" name="activeStatus" hidden>

                        </div>






                        <button type="submit" class="btn btn-success  btn-lg mt-1 ml-5"
                            style="width: 13rem;border-radius: 15px;">Sign Up </button>
                    </form>

                </div>
            </div>

        </div>
    </section>
</body>

</html>