                <section>
    <div class="container">
        <div class="row">
            <div class="col-md-6 mt-5 ">
                <h2 class="text-center">Add User</h2>
       
  <form class="needs-validation mt-4" action="/admin/addUser" method="post" novalidate>
  <div class="form-col">
    <div class="col-md-4 mb-3">
      
      <input type="text" class="form-control"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom01" name="Fname" placeholder="First name" 
        required>
      <div class="valid-feedback">
        Looks good!
      </div>
    </div>
    
    <div class="col-md-4 mb-3">
      
      <input type="text" class="form-control"   style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02"   name="Lname" placeholder="Last name" 
        required>
      <div class="valid-feedback">
        Looks good!
      </div>
    </div>
    

    <div class="col-md-4 mb-3">
      
      <input type="text" class="form-control"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02"  name="Name"  placeholder="User name" 
        required>
      
      <div class="valid-feedback">
        Looks good!
      </div>
      <div class="invalid-feedback">
        name required
      </div>
    </div>
    

    <div class="col-md-4 mb-3">
      
      <input type="email" class="form-control"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" placeholder="Email" name="Email"   
        required>
      <div class="valid-feedback">
        Looks good!
      </div>
      <div class="invalid-feedback">
        wrong email Id
      </div>
    </div>
    
    <div class="col-md-4 mb-3">
      
      <input type="text" class="form-control"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" id="validationCustom02" name="Password"    placeholder="password" 
        required>
      <div class="valid-feedback">
        Looks good!
      </div>
      <div class="invalid-feedback">
        password required!
      </div>
    </div>
    
    <div class="col-md-4 mb-3" >
      
      <input  type="tel"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;" class="form-control" name="phonenumber" id="validationCustom02" placeholder="Phone Number" 
        required>
      <div class="valid-feedback">
        Looks good!
      </div>
      <div class="invalid-feedback">
        wrong number!
      </div>
    </div>
      <input  type="text"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;"  value="true" name="blockStatus" 
        hidden>
         <input  type="text"  style="width: 20rem; border: none;
  border-bottom: 2px solid gray; border-width:2px;"  value="true" name="activeStatus" 
        hidden>
    
    

</div>
  


  
  
  
  <button type="submit" class="btn btn-success  btn-lg mt-1 ml-5" style="width: 13rem;border-radius: 15px;">Submit </button>
</form>





            </div>
        </div>
    </div>
